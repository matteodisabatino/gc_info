{
  "name": "@matteodisabatino/gc_info",
  "version": "1.0.0",
  "main": "lib/index.js",
  "scripts": {
    "install": "prebuild-install --tag-prefix \"\" || node-gyp rebuild",
    "lint": "eslint index.js test --fix",
    "prebuild": "prebuild -r node -t 5.0.0 -t 6.0.0 -t 7.0.0 -t 8.0.0 -t 9.0.0 -t 10.0.0 -t 11.0.0 -t 12.0.0 -t 13.0.0 -t 14.0.0 -t 15.0.0 -t 16.0.0 --force --strip --tag-prefix \"\"",
    "rebuild": "node-gyp rebuild",
    "release": "release-it --ci",
    "test": "npx nyc --reporter=text ava --node-arguments='--expose_gc' test/**/*.spec.js --check-coverage --lines 70"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matteodisabatino/gc_info"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "garbage collection",
    "garbage collection info",
    "garbage collection information",
    "garbage collection statistics",
    "garbage collection stats",
    "garbage collector",
    "garbage collector info",
    "garbage collector information",
    "garbage collector statistics",
    "garbage collector stats",
    "gc",
    "gc info",
    "gc information",
    "gc statistics",
    "gc stats",
    "heap",
    "heap info",
    "heap information",
    "heap statistics",
    "heap stats",
    "v8",
    "v8 info",
    "v8 information",
    "v8 statistics",
    "v8 stats",
    "v8 heap",
    "v8 heap info",
    "v8 heap information",
    "v8 heap statistics",
    "v8 heap stats"
  ],
  "author": "Matteo Di Sabatino (@matteodisabatino)",
  "license": "ISC",
  "description": "Exposes node v8 garbage collection stats",
  "dependencies": {
    "bindings": "^1.5.0",
    "nan": "^2.14.2",
    "prebuild-install": "^6.0.1"
  },
  "gypfile": true,
  "binary": {
    "module_name": "gc_info"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^3.3.0",
    "ava": "^3.15.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-ava": "^13.1.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "prebuild": "^11.0.0",
    "release-it": "^14.4.1",
    "semver": "^7.3.5"
  },
  "engines": {
    "node": "<=16"
  }
}
